{
  "name": "@substrateos/free",
  "version": "0.1.0",
  "description": "SubstrateOS Free - Browser-native Linux playground",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "vite",
    "preview": "vite preview"
  },
  "keywords": [
    "linux",
    "shell",
    "browser",
    "sandbox",
    "education",
    "free"
  ],
  "author": "Edwards Tech Innovation",
  "license": "Apache-2.0",
  "homepage": "https://substrateos.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/edwards-tech/substrateos"
  },
  "peerDependencies": {
    "@substrateos/runtime": "^0.1.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  },
  "tier": {
    "name": "free",
    "price": "$0",
    "features": {
      "shell": true,
      "sqlite": true,
      "tutorials": true,
      "python": false,
      "nodejs": false,
      "agentSdk": false,
      "embedding": false
    }
  }
}
